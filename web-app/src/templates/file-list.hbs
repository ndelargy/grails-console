<div class="files-page-header">
    <h1>Local Files</h1>
</div>

<table class="table table-striped table-hover">
  <thead>
    <tr>
        <th></th>
        <th>Name</th>
        <th>Date Modified</th>
    </tr>
  </thead>
  {{#each this}}
    <tr data-file-id="{{id}}">
        <td><a class="delete" href="#"><i class="icon-trash"></i></a></td>
        <td><a class="name" href="#">{{name}}</a></td>
        <td>{{dateFormatTime lastModified}}</td>
    </tr>
  {{/each}}
</table>